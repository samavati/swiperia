{
  "name": "swiperia-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/swiperia-react/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project swiperia-react --web",
  "targets": {
    "nx-release-publish": {
      "dependsOn": [
        "^nx-release-publish"
      ],
      "executor": "@nx/js:release-publish",
      "options": {},
      "configurations": {}
    },
    "build": {
      "options": {
        "cwd": "packages/swiperia-react",
        "command": "vite build"
      },
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "vite"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/dist/packages/swiperia-react"
      ],
      "executor": "nx:run-commands",
      "configurations": {}
    },
    "serve": {
      "options": {
        "cwd": "packages/swiperia-react",
        "command": "vite serve"
      },
      "executor": "nx:run-commands",
      "configurations": {}
    },
    "preview": {
      "options": {
        "cwd": "packages/swiperia-react",
        "command": "vite preview"
      },
      "executor": "nx:run-commands",
      "configurations": {}
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "build",
        "spa": true
      },
      "configurations": {}
    },
    "test": {
      "options": {
        "cwd": "packages/swiperia-react",
        "command": "vitest run"
      },
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "vitest"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/coverage/packages/swiperia-react"
      ],
      "executor": "nx:run-commands",
      "configurations": {}
    },
    "lint": {
      "cache": true,
      "options": {
        "cwd": "packages/swiperia-react",
        "command": "eslint ."
      },
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/.eslintrc.base.json",
        "{projectRoot}/.eslintrc.json",
        "{workspaceRoot}/tools/eslint-rules/**/*",
        {
          "externalDependencies": [
            "eslint"
          ]
        }
      ],
      "executor": "nx:run-commands",
      "configurations": {}
    }
  },
  "implicitDependencies": []
}