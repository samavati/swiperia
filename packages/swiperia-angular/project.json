{
  "name": "swiperia-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/swiperia-angular/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "nx-release-publish": {
      "dependsOn": [
        "^nx-release-publish"
      ],
      "executor": "@nx/js:release-publish",
      "options": {},
      "configurations": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {},
      "configurations": {}
    },
    "build": {
      "executor": "@nx/angular:package",
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "default",
        "^default"
      ],
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "packages/swiperia-angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/swiperia-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/swiperia-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ],
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "passWithNoTests": true,
        "jestConfig": "packages/swiperia-angular/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "implicitDependencies": []
}